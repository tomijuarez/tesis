      <output message='tns:createSettlementResponseMessage' name='createSettlementOutput' />
    </operation>
    <operation name='deleteASettlement'>
      <documentation>delete the specified settlement run Note this is only possible before the settlement run is released</documentation>
      <input message='tns:deleteASettlementRequestMessage' name='deleteASettlementInput' />
      <output message='tns:deleteASettlementResponseMessage' name='deleteASettlementOutput' />
    </operation>
