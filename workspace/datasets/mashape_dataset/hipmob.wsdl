      <output message='tns:removeDeviceFriendResponseMessage' name='removeDeviceFriendOutput' />
    </operation>
    <operation name='removeDeviceFriends'>
      <documentation>clears the friend list of the specified device the device will no longer be able to send messages to any other devices or receive messages from any other devices</documentation>
      <input message='tns:removeDeviceFriendsRequestMessage' name='removeDeviceFriendsInput' />
      <output message='tns:removeDeviceFriendsResponseMessage' name='removeDeviceFriendsOutput' />
    </operation>
    <operation name='sendTextMessage'>
      <documentation>sends a text message to the specified device</documentation>
      <input message='tns:sendTextMessageRequestMessage' name='sendTextMessageInput' />
      <output message='tns:sendTextMessageResponseMessage' name='sendTextMessageOutput' />
    </operation>
    <operation name='setDeviceFriends'>
      <documentation>clears the friends list and then adds the specified friends to the friends list of a specific device Equivalent to calling Remove All Device Friends followed by Add Device Friends</documentation>
